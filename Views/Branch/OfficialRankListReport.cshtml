@model IEnumerable<ODHDEVELOPERS.Models.Ranktab>
@using ODHDEVELOPERS.Models
@{
    UsersContext db = new UsersContext();
    var mr = db.Members.Single(m => m.Id == 1);
    Layout = "~/Views/Shared/_BranchLayout.cshtml";
    ViewBag.Title = "OfficialRankListReport";
    var a = 0;
}

<script src="~/Scripts/print.js" type="text/javascript"></script>
<div class="page-content">
      <div style="margin-left:25px;">
   <a href="@Url.Action("PrintRank")" target="_blank">Get Report In PDF</a></div>
<div class="container" >
                  
<div class="page-toolbar" >
                        
                        <div class="page-toolbar-block">
                            <div class="page-toolbar-title">@mr.rankname List</div>
                            <div class="page-toolbar-subtitle"> </div>
                        </div>
                               <div class="page-toolbar-block pull-right">
                            
                        </div>    
                       <table border="1" style="margin-left:1%; margin-right:1%; margin-bottom:30px; width:98%;">
   
    @foreach(var item in Model){
        if(a==0)
        {
  <tr>
        <th style="width:30%;text-align:center;">
           <label id="sn"><b>@mr.rankname No</b></label>
            </th>
      
        <th style="width:30%;text-align:center;">
           <label id="introducerid"><b>@mr.rankname Name</b></label>
        </th>
            
       <th style="width:30%;text-align:center;">
           <label id="Quota"><b>@mr.rankname Shortcut</b></label>
        </th>
    </tr>
        }

        a = a + 1;
    <tr style="text-align:center;">
        <td>
            @a
        </td>
      
        
        <td>
            @Html.DisplayFor(modelItem => item.RankName)
        </td>
  <td>
            @Html.DisplayFor(modelItem => item.Rankshrtcut)
        </td>
             
    </tr>
    } 

</table>
                    </div> 


                   
      

      
                    </div>


    </div>


