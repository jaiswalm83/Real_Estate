        margin-top: -35px;
        margin-right: -15px;
    }

    .widget-info {
        background-color: #f4f4f4;
        padding: 10px;
        border-radius: 5px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    }

    .page-toolbar-block {
        text-align: right;
        font-size: 14px;
        font-family: Arial, sans-serif;
    }

    label {
        color: #333;
        font-weight: bold;
        padding-right: 5px;
    }

   
