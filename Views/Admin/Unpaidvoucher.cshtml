@model IEnumerable<ODHDEVELOPERS.Models.PaidVoucher>
@using ODHDEVELOPERS.Models;
@{
    UsersContext db = new UsersContext();
    var mr = db.Members.Single(m => m.Id == 1);
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "Unpaidvoucher";
    var a = 0;
    var bsum = 0.0;
    var comsum = 0.0;
    var tdsum = 0.0;
    var netsum = 0.0;
    var netadvamount = 0.0;
}

<div class="page-content">
    
                <div class="container" id="divid">
                  
<div class="page-toolbar">
                        
                        <div class="page-toolbar-block">
                            <div class="page-toolbar-title">Unpaid Voucher</div>
                            <div class="page-toolbar-subtitle"> </div>
                        </div>
                               <div class="page-toolbar-block pull-right">
                           
                        </div>    
                              

                    </div> 

                     @using (@Html.BeginForm()){
                    <div class="row" >
                        <div class="col-md-6" style="width:98%;">
                            
                            <div class="block" >
                               
                                <div class="block-content controls">
                                      
                               
                                     <div class="row-form">
           <div class="input-group" style="margin-left:1%;margin-right:1%;">
                        <span class="input-group-addon"><label class="width">Select Month</label></span>
                         <select class="form-control" id="month" name="month" required>
                                            <option selected>Select </option>
                                                  <option value="1">January</option>
                                                  <option value="2">February</option>
                                                  <option value="3">March</option>
                                                  <option value="4">April</option>
                                                  <option value="5">May</option>
                                                  <option value="6">June</option>
                                                  <option value="7">July</option>
                                                  <option value="8">August</option>
                                                  <option value="9">September</option>
                                                  <option value="10">October</option>
                                                  <option value="11">November</option>
                                                  <option value="12">December</option>
                                                

                                        </select>
                </div>   </div>

                                    <div class="row-form">
           <div class="input-group" style="margin-left:1%;margin-right:1%;">
                        <span class="input-group-addon"><label class="width">Select Year</label></span>
                         <select class="form-control" id="year" name="year" required>
                                            <option selected>Select </option>
                                                  <option>2013 </option>
                                                  <option>2014 </option>
                                                  <option>2015 </option>
                                                  <option>2016 </option>
                                                  <option>2017 </option>
                                                  <option>2018 </option>
                                                  <option>2019 </option>
                                                  <option>2020 </option>
                                                  <option>2021 </option>
                                                  <option>2022 </option>
                                                  
                                                

                                        </select>
                </div>   </div>
                                     
                                       <div class="page-toolbar-block pull-right">
                            <div class="widget-info widget-from">
                                                          
                               <button id="btnsearch" name="command" type="submit" class="btn btn-primary" value="Search">Search</button>                            
                                
                            </div>
                        </div>  
                                   </div>           
                        
                    </div>  
                             </div>    </div>
                    }



<table border="1" style="margin-left:1%; margin-right:1%; margin-bottom:30px; width:98%">
   
    @foreach (var item in Model)
    {
        if (a == 0)
        {
             <tr><td colspan="3"  style="width:25%;text-align:left;font-size:medium;height:50px;"> <label id="dddd"><b>Branch Name: @ViewData["Branch"] </b></label></td><td colspan="3" style="width:75%;text-align:center;font-size:medium;height:50px;"><b>Unpaid Voucher Report</b></td><td colspan="4"  style="width:75%;text-align:center;font-size:medium;height:50px;"><b>Month: @ViewData["Month"] </b></td></tr>        
  <tr>
        <th style="width:5%;text-align:center;">
           <label id="sn"><b>S.N.</b></label>
            </th>
        <th style="width:10%;text-align:center;">
           <label id="OperatorId"><b>@mr.agentname Id</b></label>
            </th>
        
       <th style="width:15%;text-align:center;">
           <label><b>@mr.agentname Name</b></label>
        </th>
              <th style="width:10%;text-align:center;">
           <label><b>PAN No</b></label>
        </th>
      <th style="width:10%;text-align:center;">
           <label><b>Business</b></label>
        </th>
            
        <th style="width:10%;text-align:center;">
            <label><b>BDP</b></label>
        </th>
            
        <th style="width:10%;text-align:center;">
            <label><b>Bonus</b></label>
        </th>            
        <th style="width:10%;text-align:center;">
           <label><b>TDS</b></label>
        </th>
       <th style="width:10%;text-align:center;">
           <label><b>Advance Amt.</b></label>
        </th>
       <th style="width:10%;text-align:center;">
           <label><b>Net Amount</b></label>
        </th>

       
       
    </tr>
        }

        a = a + 1;
    <tr style="text-align:center;">
        <td>
            @a
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Brokercode)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.BrokerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.panno)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.bussiness)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.commission)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.bonusamount)
        </td>         
        <td>
            @Html.DisplayFor(modelItem => item.tds)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.advamount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.netamount)
        </td>
        
        
    </tr>
        
        bsum = bsum + Convert.ToDouble(item.bussiness);
        comsum = comsum + Convert.ToDouble(item.commission);
        tdsum = tdsum + Convert.ToDouble(item.tds);
        netsum = netsum + Convert.ToDouble(item.netamount);
        netadvamount = netadvamount + Convert.ToDouble(item.advamount);
       
    } 
    @if(a>0)
     {
     <tr style="text-align:center"><td colspan="4"></td><td>@bsum</td><td>@comsum</td><td>@Model.Select(d=>d.bonusamount).DefaultIfEmpty(0).Sum()</td><td>@tdsum</td><td>@netadvamount</td><td>@netsum</td></tr>
     }

</table>
                   
 @if(a>0)
     {
       <a href="@Url.Action("Printunpaidvoucher")" target="_blank">Get Report In PDF</a>
     }
          
      
                    </div>


    </div>





