@using ODHDEVELOPERS.Models;
@{
    UsersContext db = new UsersContext();
    var mr = db.Members.Single(m => m.Id == 1);   
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "DeleteCommission";
   
}
<link rel="stylesheet" href="~/Content/jquery-ui.css" />

<script type="text/javascript">
    $(document).ready(function () {
        $("#BondId").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "/Branch/AutoCompleteBondId",
                    type: "POST",
                    dataType: "json",
                    data: { term: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return { label: item.name, value: item.newbondid };
                        }))

                    }
                })
            },
            messages: {
                noResults: "", results: ""
            }
        });
    })
</script>



<div class="page-content">

                <div class="container">

                    <div class="page-toolbar">
                        
                        <div class="page-toolbar-block">
                            <div class="page-toolbar-title">Delete Commission</div>
                            <div class="page-toolbar-subtitle"> </div>
                        </div>
                        
                        <div class="page-toolbar-block pull-right">
                            
                        </div>           
                        
                    </div>  

                           <div class="row" >
                                @using (Html.BeginForm())
               {   
                        <div class="col-md-6" style="width:100%">
                            
                            <div class="block" >
                               
                                <div class="block-content controls">
                                   <div class="row-form">  
                                         
                                    <div class="col-md-9">
                                      <div class="input-group">
                                    <span class="input-group-addon"><label class="width">@mr.custname Id or Name</label></span>  
                                    <input type="text"  class="form-control"  name="newbondid" id="BondId" ></div>
                                   
                                        </div>
                                   </div>
                                   <div class="page-toolbar-block pull-right">
                            <div class="widget-info widget-from">
                                                          
                                <button type="submit"  class="btn btn-primary"  id="search" onclick="return confirm('Are you sure to delete commission of this @mr.custname??')">Delete Commission</button>                            
                               
                            </div>
                        </div>  
                                     
                                 
                                </div>
                                
                            </div>
                            </div>
                                }
                              
                        
                    </div>
  
    </div>
    </div>



