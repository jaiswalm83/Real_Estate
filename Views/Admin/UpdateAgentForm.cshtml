<script type="text/javascript">
    $(function () {
        (function () {
            var v = $('[name="RankName"]').attr('data-v');
            $('[name="RankName"] option').each(function () {
                var c = $(this), cv = c.val();
                if (cv == v) {
                    c.attr('selected', '');
                    return false;
                }
            });
        })();
    });
</script>