@using ODHDEVELOPERS.Models;
@{
    UsersContext db = new UsersContext();
    var br = db.CompanyInfos.Single(b => b.AdminId == User.Identity.Name);
    var logo = string.Empty;
    var companyname = string.Empty;
    var incometype = string.Empty;
    var mr = db.Members.Single(m => m.Id == 1);
    var r = db.CompanyInfos.Single(ci => ci.AdminId == User.Identity.Name);


    logo = r.CompanyLogo;
    companyname = r.CompanyName;
    incometype = r.IncomeType;

    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.Title = "Create";

}
<link href="~/assets/css1/main css/Pages.css" rel="stylesheet" />

<!-- Section 1: Final Report -->
<div class="section">

    <div class="w-100 card-heading">
        <h3 class="text-center text-white pt-2 pb-2">Create</h3>
    </div>
   

    <div class="row g-3">
        <div class="container mt-5">
            <!-- Cards Row -->
            <div class="row g-3">
                <div class="col-sm-6 col-md-4 col-lg-2 mb-3">
