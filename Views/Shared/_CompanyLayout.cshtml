@using ODHDEVELOPERS.Models;
@{
    UsersContext db=new UsersContext();
    int count = (from ob in db.CompanyInfos select ob).Count();
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Company Panel</title>

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link href="~/Content/Companywidth.css" rel="stylesheet" />
    <link href="~/Content/css/operatorstyle.css" type="text/css" rel="stylesheet" />
    <link href="~/Content/css/styles.css" rel="stylesheet" type="text/css" id="theme" />
    <script type="text/javascript" src="~/Scripts/js/plugins/jquery/jquery.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/jquery/jquery-ui.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/bootstrap/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/mcustomscrollbar/jquery.mCustomScrollbar.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/sparkline/jquery.sparkline.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=false&amp;libraries=places"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/fancybox/jquery.fancybox.pack.js"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/rickshaw/d3.v3.js"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/rickshaw/rickshaw.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/flot/jquery.flot.js"></script>
    <script type='text/javascript' src='~/Scripts/js/plugins/flot/excanvas.min.js'></script>
    <script type='text/javascript' src='~/Scripts/js/plugins/flot/jquery.flot.js'></script>
    <script type='text/javascript' src='~/Scripts/js/plugins/flot/jquery.flot.stack.js'></script>
    <script type='text/javascript' src='~/Scripts/js/plugins/flot/jquery.flot.pie.js'></script>
    <script type='text/javascript' src='~/Scripts/js/plugins/flot/jquery.flot.resize.js'></script>
    <script type='text/javascript' src='~/Scripts/js/plugins/knob/jquery.knob.js'></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/daterangepicker/moment.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/plugins/daterangepicker/daterangepicker.js"></script>
    <script type='text/javascript' src='~/Scripts/js/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js'></script>
    <script type='text/javascript' src='~/Scripts/js/plugins/jvectormap/jquery-jvectormap-europe-mill-en.js'></script>
    <script type="text/javascript" src="~/Scripts/js/plugins.js"></script>
    <script type="text/javascript" src="~/Scripts/js/demo.js"></script>
    <script type="text/javascript" src="~/Scripts/js/maps.js"></script>
    <script type="text/javascript" src="~/Scripts/js/charts.js"></script>
    <script type="text/javascript" src="~/Scripts/js/actions.js"></script>
    <link href="~/Content/datepicker/jquery.datepick.css" rel="stylesheet" />
<script src="~/Content/datepicker/jquery.plugin.js"></script>
<script src="~/Content/datepicker/jquery.datepick.js"></script>
</head>
<body>
    <div class="page-container">

        <header>
            <div class="page-head">

                <ul class="page-head-elements">
                    <li><a href="#" class="page-navigation-toggle"><span class="fa fa-bars"></span></a></li>
                    <li class="search">
                        <input type="text" class="form-control" placeholder="Search..." />
                    </li>
                </ul>

                <ul class="page-head-elements pull-right">
                    <li>
                        <div class="informer informer-danger informer-pulsate">!</div>
                        <a href="#" class="dropdown"><span class="fa fa-tint"></span></a>
                        <div class="popup themes">
                            <div class="title">Themes</div>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/styles.css'); return false;">
                                <img src="~/Content/img/themes/default.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-d-hw.css'); return false;">
                                <img src="~/Content/img/themes/d-hw.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-d-orange.css'); return false;">
                                <img src="~/Content/img/themes/d-orange.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-d-hw-orange.css'); return false;">
                                <img src="~/Content/img/themes/d-hw-orange.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-d-hg-lred.css'); return false;">
                                <img src="~/Content/img/themes/d-hg-lred.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-d-hw-turquoise.css'); return false;">
                                <img src="~/Content/img/themes/d-hw-turquoise.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-d-hb-purple.css'); return false;">
                                <img src="~/Content/img/themes/d-hb-purple.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-dg-hg-blue.css'); return false;">
                                <img src="~/Content/img/themes/dg-hg-blue.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-lb-hdb-lb.css'); return false;">
                                <img src="~/Content/img/themes/lb-hdb-lb.jpg" /></a>
                            <a href="#" class="theme-item" onclick="theme($(this),'~/Content/css/theme-dg-hb-blue.css'); return false;">
                                <img src="~/Content/img/themes/dg-hb-blue.jpg" /></a>

                            <p><span class="fa fa-info-circle"></span>LessCss files included in package!</p>
                        </div>
                    </li>
                    <li>
                        <a href="#" class="dropdown"><span class="fa fa-comments"></span></a>
                        <div class="popup">
                            <div class="list no-controls">
                                <a href="#" class="list-item">
                                    <div class="list-item-content">
                                        <h4>John Doe</h4>
                                        <p>
                                            <img src="~/Content/img/samples/users/user-30.jpg" class="~/Content/img-circle pull-left" />
                                            Lorem ipsum dolor sit amet, consectetur adipiscing elit?
                                        </p>
                                        <span class="date">15:54</span>
                                    </div>
                                </a>
                                <a href="#" class="list-item">
                                    <div class="list-item-content">
                                        <h4>Tom Cruise</h4>
                                        <p>
                                            <img src="~/Content/img/samples/users/ben.jpg" class="~/Content/img-circle pull-left" />
                                            Nulla non venenatis nisl, nec sodales dolor
                                        </p>
                                        <span class="date">12:01</span>
                                    </div>
                                </a>
                                <a href="#" class="list-item">
                                    <div class="list-item-content">
                                        <h4>Jonny Lee Miller</h4>
                                        <p>
                                            <img src="~/Content/img/samples/users/jonny.jpg" class="~/Content/img-circle pull-left" />
                                            Ut leo ante, sodales vitae semper
                                        </p>
                                        <span class="date">10:25</span>
                                    </div>
                                </a>
                                <a href="#" class="list-item">
                                    <div class="list-item-content">
                                        <h4>Emma Watson</h4>
                                        <p>
                                            <img src="~/Content/img/samples/users/liu.jpg" class="~/Content/img-circle pull-left" />
                                            Maecenas non felis tincidunt, rhoncus risus...
                                        </p>
                                        <span class="date">10:18</span>
                                    </div>
                                </a>
                                <a href="#" class="list-item">
                                    <div class="list-item-content">
                                        <h4>Martin Freeman</h4>
                                        <p>
                                            <img src="~/Content/img/samples/users/martin.jpg" class="~/Content/img-circle pull-left" />
                                            Maecenas non felis tincidunt, rhoncus risus
                                        </p>
                                        <span class="date">9:32</span>
                                    </div>
                                </a>
                            </div>
                            <div class="popup-block tac"><a href="pages-mailbox-inbox.html">Show all messages</a></div>
                        </div>
                    </li>

                    <li><a href="#" class="page-sidebar-toggle"><span class="fa fa-tasks"></span></a></li>
                </ul>

            </div>




            <div class="page-navigation">

                <div class="page-navigation-info">
                    <div style="float: left;">
                        <img id="imgid" src="~/Photo/Logo.png " style="height: 50px; width: 40px;" />
                    </div>
                    <div style="float: left;">
                        <a href="~/Admin/Index">
                            <h5 style="color: white;"><b>XPERT GROUP</b></h5>
                        </a>
                    </div>
                </div>

                <div class="profile">
                    <div class="profile-info">
                        <div class="profile-title" style="text-transform: uppercase; margin-left: 5px; float: left; font-size: small;">
                            <h6>Hello , @User.Identity.Name</h6>
                        </div>
                        <div class="profile-buttons">
                            <div class="btn-group">
                                <a class="but dropdown-toggle" data-toggle="dropdown"><i class="fa fa-cog"></i></a>
                                <ul class="dropdown-menu" role="menu">
                                    <li role="presentation" class="dropdown-header">Profile</li>
                                    <li class="divider"></li>
                                    <li><a href="~/Company/ChangePassword">Change Password</a></li>
                                    <li class="divider"></li>
                                    <li><a href="~/Company/Logout">Logout</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>

                <ul class="navigation">
                    <li><a href="~/Company/Index"><i class="fa fa-home"></i>Home</a></li>
                    <li><a href="#"><i class="fa fa-plus"></i>Company </a>
                        <ul>
                            @if (count == 0)
                            {
                                <li><a href="~/Company/CompanyReg"><i class="fa fa-caret-right"></i>Create</a></li>
                            }
                            else if (count > 0)
                            {
                                <li><a href="~/Company/UpdateCompany"><i class="fa fa-caret-right"></i>Update</a></li>
                            }
                        </ul>
                    </li>
                    <li><a href="#"><i class="fa fa-pencil"></i>Update</a>
                        <ul>

                            <li><a href="~/Company/UpdateMember"><i class="fa fa-caret-right"></i>Member</a></li>
                            <li><a href="~/Company/UpdateVoucherName"><i class="fa fa-caret-right"></i>Voucher Name</a></li>


                        </ul>
                    </li>

                    <li><a href="#"><i class="fa fa-cogs"></i>Setting</a>
                        <ul>
                            <li><a href="~/Company/MacAddress"><i class="fa fa-caret-right"></i>Mac Address</a></li>
                            <li><a href="~/Company/LoginDetail"><i class="fa fa-caret-right"></i>Login Details</a>
                            </li>
                            <li><a href="~/Company/ApproveWebsite"><i class="fa fa-caret-right"></i>Approve PC</a></li>
                        </ul>
                    </li>

                </ul>

            </div>
        </header>


        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>


        <!--=== Footer ===-->
        <footer>
        </footer>
    </div>


</body>
</html>